	<ItemGroup>
		<PackageReference Include="Microsoft.Data.SqlClient" Version="6.1.4" />
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="[9.*,)" />
	</ItemGroup>

</Project>
